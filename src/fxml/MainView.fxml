<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="599.0" prefWidth="1433.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainViewController">
    <children>
      <BorderPane fx:id="pane" layoutX="21.0" layoutY="115.0" prefHeight="573.0" prefWidth="643.0" />
      <TabPane layoutX="664.0" layoutY="19.0" onMouseClicked="#refreshSunburstView" onMousePressed="#refreshSunburstView" prefHeight="658.0" prefWidth="660.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Folders">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <fx:include fx:id="foldersTab" source="FoldersTab.fxml" />
                     </children>
                  </AnchorPane>
               </content>
          </Tab>
          <Tab text="Files">
               <content>
                  <fx:include fx:id="fileTab" source="FileTab.fxml" />
               </content>
          </Tab>
        </tabs>
      </TabPane>
      <Pane layoutX="14.0" layoutY="19.0" prefHeight="107.0" prefWidth="619.0">
         <children>
            <fx:include fx:id="switches" source="Switches.fxml" />
         </children>
      </Pane>
    </children>
</AnchorPane>
